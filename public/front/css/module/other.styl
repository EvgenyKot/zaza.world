                height: 150px;
                margin: auto;
                border-radius: 50%;
                overflow: hidden;
                border: 3px solid #15499f;
                & img
                    width: 100%;
            & .p_p_location
                margin: 10px 0 0 0;
                font-size: 1.1em;
                & img
                    width: auto;
            & .p_p_about
                visibility: visible;
                &__title
                    font-size: 1.4em
                    font-weight: 600;
                    margin-bottom: 10px;
                    display: block;
            & .p_p_phones
                font-size: 1.15em;
                & img
                    width: auto;
                    margin-right: 8px;
            & .p_p_social
                font-size: 1.2em;
                &__text
                    display: inline-block;
                    position: relative;
                    top: -6px;
                & img
                    width: auto;
                    height: auto;
                    max-height: 25px;
                    margin-right: 8px;
        & .p_personal__left_tab
            margin-top: 25px;
            &__li
                display: block;
                width: 100%;
                margin-bottom: 2px;
                &_title
                    display: block;
                    width: 130%;
                    padding: 6px 0;
                    background: #15499f;
                    color: #fff;
                    position: relative;
                    left: -30%;
                    padding-left: 30%;
                    font-size: 1.2em;
                    cursor: pointer;
                &_open
                    content: ' + '
                    display: block;
                    float: right;
                    width: 23px;
                    height: 23px;
                    color: #fff;
                    margin-right: 10px;
                    border: 1px solid #fff;
                    text-align: center;
                    border-radius: 50%;
                    &.active
