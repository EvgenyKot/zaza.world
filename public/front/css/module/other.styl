

/********* snippets *****/
.shadow
    box-shadow: 0 0 10px rgba(0,0,0,0.5);


/*!****** total blur ****/
.total_blur
    z-index: 50;
    -webkit-filter: blur(3px);
    -ms-filter: blur(3px);
    -moz-filter: blur(3px);
    -o-filter: blur(3px);
    filter: blur(3px);
    filter: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg'><filter id='blur'><feGaussianBlur stdDeviation='3'/></filter></svg>#blur");

/*!****** total blur ****/
.normal_blur
    z-index: 1;
    -webkit-filter: blur(3px);
    -ms-filter: blur(3px);
    -moz-filter: blur(3px);
    -o-filter: blur(3px);
    filter: blur(3px);
    filter: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg'><filter id='blur'><feGaussianBlur stdDeviation='3'/></filter></svg>#blur");



.p_conteiner
    width: 100%;
    margin: 10px auto;
    overflow: hidden;
    max-width: 980px;
/*!************ title block  ************/
.p_title
    margin: 77px 0 0 0;
    height: 116px;
    box-shadow: 0px 5px 100px -30px #000;
    padding-top: 20px;
    &__name
        float: left;
        margin: 16px 0 0 0;
        font-size: 2.2em;
    &__buttons
        float: right;
        margin: 17px 0 0 0;
    &__button
        float: left;
        margin-left: 15px;
        height: 20px
        &__chat
            visibility: visible;
            &__img
                width: auto;
                height: 100%;
                top: 5px;
                position: relative;
                margin-right: 6px;
            &__text
                font-size: 1.5em;
                border-bottom: 1px dotted #263238;
        &__onlain
            font-size: 0.9em;
            width: 130px;
            &__circle
                height: 20px;
                width: 20px;
                display: block;
                float: left;
                margin: 5px 9px 5px 5px;
                border-radius: 50%;
                overflow: hidden;
                background: #d41f26;
            &.active &__circle
                background: #8bc34a;

.p_personal
    max-width: 980px;
    width: 100%;
    margin: 10px auto;
    padding: 20px 0;
    &__left
        float: left;
        width: 30%;
        border-right: 1px solid #15499f;

        &_block
            padding-top: 25px;
            padding-right: 10px
